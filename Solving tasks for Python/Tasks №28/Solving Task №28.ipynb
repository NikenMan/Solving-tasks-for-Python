    "def factors(num, d=2):\n",
    "    while num > 1:\n",
    "        n1, n2 = divmod(num, d)\n",
    "        if n2:\n",
    "            d += 1\n",
    "        else:\n",
    "            yield d\n",
    "            num = n1\n",
    "\n",
    "n = int(input(\"Integer: \"))\n",
    "print('{} = {}' .format(n, ' * '.join(map(str, factors(n)))))\n"
   ],
