    "def main():\n",
    "    print \"Program calculates the area of a triangle.\"\n",
    "    print\n",
    "    a, b, c = input(\"Enter triangle's sides length: \")\n",
    "    s = (a+b+c) / 2.0\n",
    "    area = sqrt(s*(s-a)*(s-b)*(s-c))\n",
    "    print \"The area is %.2f\" % area\n",
    "\n",
    "main()"
   ]
  }
